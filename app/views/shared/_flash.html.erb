<% if flash[:notice] %>
    <div class="alert alert-dark mt-4 alert-dismissible fade show" role="alert" ><%= flash[:notice] %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
</div>
<% end %>

<% if flash[:alert] %>
    <div class="alert alert-dark mt-4 alert-dismissible fade show" role="alert" ><%= flash[:alert] %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
</div>
<% end %>
